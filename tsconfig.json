{
    "compilerOptions": {
        "jsx": "react",
        "target": "ES2022",
        "module": "ES2022",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "strict": true,
        "incremental": true,
        "baseUrl": "types",
        "typeRoots": ["types"]
    },
}